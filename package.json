{
  "name": "seisplotjs",
  "version": "2.0.0-alpha.2",
  "description": "Javascript library for parsing, manipulating and displaying seismic data.",
  "main": "lib/index.js",
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "crc-32": "^1.2.0",
    "d3": "^5.9.7",
    "js-quantities": "^1.7.4",
    "moment": "^2.24.0",
    "oregondsp": "^1.1.0",
    "pikaday": "^1.8.0",
    "rsvp": "^4.8.5"
  },
  "scripts": {
    "lint": "eslint 'src/*.js' 'src/*/*.js' 'test/*.js' 'test/*/*.js' ",
    "examplelint": "eslint  'example/socal/socal.js' 'example/southcarolina/southcarolina.js' ",
    "test": "jest",
    "flow": "flow",
    "flow-copy": "flow-copy-source -v -i '**/__tests__/**' src lib",
    "build:clean": "rimraf lib",
    "build:compile": "babel  -d lib/ src/ ",
    "compile": "npm run build:clean && npm run build:compile && npm run flow-copy",
    "standalone": "browserify src/index.js --ignore-missing --standalone seisplotjs -o seisplotjs_${npm_package_version}_standalone.js  -t [ babelify ]",
    "doc": "documentation build  src/*.js --github -o docs -f html --document-exported ",
    "apidoc": "npx documentation build -f html -o docs/docall --document-exported --github src/[a-h]*.js src/[j-z]*.js  src/filter/[a-h]*.js src/filter/[j-z]*.js src/waveformplot/[a-h]*.js src/waveformplot/[j-z]*.js",
    "prepare": "npm run compile",
    "version": "npm run compile && npm run standalone",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs.git"
  },
  "keywords": [
    "seismology",
    "miniseed",
    "steim1",
    "steim2"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "documentation": "^11.0.1",
    "esdoc": "^1.1.0",
    "esdoc-flow-type-plugin": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.102.0",
    "flow-copy-source": "^2.0.6",
    "flow-remove-types": "^2.102.0-rc",
    "flowtypify": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "jest-extended": "^0.11.1",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 1% in US",
              "not ie 11",
              "not dead"
            ],
            "node": "current"
          }
        }
      ],
      "@babel/preset-flow"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
