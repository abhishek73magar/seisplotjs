{
  "name": "seisplotjs",
  "version": "3.0.0-alpha.3",
  "description": "Javascript library for parsing, manipulating and displaying seismic data.",
  "module": "./dist/index.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "browser": "./dist/index.esm.js",
  "types": "./dist/dts/index.d.js",
  "sideEffects": false,
  "type": "module",
  "files": [
    "./dist/index.js",
    "./dist/index.esm.js",
    "./dist/index.d.js",
    "src"
  ],
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "convert-units": "^3.0.0-beta.4",
    "crc-32": "^1.2.1",
    "d3": "^6.6.2",
    "handlebars": "^4.7.7",
    "jszip": "^3.7.1",
    "leaflet": "^1.7.1",
    "luxon": "^3.0.1",
    "oregondsp": "^1.3.0-beta.2",
    "querystringify": "^2.2.0"
  },
  "scripts": {
    "esbuild": "cp src/handlebarsimport.esbuild src/handlebarsimport.ts && npx esbuild src/index.ts --bundle --outfile=dist/seisplotjs_${npm_package_version}_standalone.mjs  --target=es2020 --format=esm",
    "esmb": "cp src/handlebarsimport.esbuild src/handlebarsimport.ts && node build.mjs",
    "esm": "npx esbuild src/*.ts --outdir=dist/module  --target=es2020 --format=esm",
    "srclint": "eslint 'src/*.js' ",
    "lint": "eslint src test --ext .js,.jsx,.ts,.tsx ",
    "examplelint": "eslint -c .eslintrc.js 'docs/examples/viewobspy/www/[otv]*.js' 'docs/examples/*/[a-r]*.js'  'docs/examples/*/s[t-z]*.js'",
    "test": "jest --config=jest.config.cjs",
    "livetest": "jest --config=jest.config.live.cjs",
    "build:clean": "rimraf dist",
    "apidoc:clean": "rimraf docs/api",
    "compile": "npm run build:clean && npm run version_to_src && npm run esmb && npm run standalone && npm run dts",
    "tutorial_snippet": "python3 snippetToTutorial.py",
    "standalone": "npm run esbuild && cp dist/seisplotjs_${npm_package_version}_standalone.mjs docs/.",
    "doc": "npm run standalone && npm run apidoc && npm run tutorial_snippet ",
    "apidoc": "npm run apidoc:clean && ./createApiDocs.sh",
    "prepare": "npm run compile",
    "version": "npm run compile && npm run standalone && npm run bumpversion && npm run doc",
    "version_to_src": "genversion --semi --es6 src/version.ts",
    "postversion": "echo remember: git push && echo remember: git push --tags",
    "bumpversion": "npm run bumpversion_standalone && npm run bumpversion_version",
    "bumpversion_standalone": "replace-in-file seisplotjs_3.0.0-alpha.2_standalone.mjs seisplotjs_${npm_package_version}_standalone.mjs docs/index.html docs/gallery/*.html docs/tutorial/*.html docs/examples/*/*.html docs/examples/*/*/*.html examples/*/*.html --verbose ",
    "bumpversion_version": "replace-in-file 3.0.0-alpha.2 ${npm_package_version} createApiDocs.sh src/index.js docs/index.html docs/tutorial/*.html  docs/tutorial/*.js docs/examples/*.html docs/examples/*/*.html docs/examples/*/*/*.html --verbose ",
    "doingdevelopment": "git update-index --skip-worktree docs/api/*.html",
    "prepublishOnly": "npm run lint && npm run test && npm outdated && git update-index --no-skip-worktree docs/api/*.html  && git diff --exit-code",
    "servedocs": "npx http-server ./docs -c-1 --cors -a 127.0.0.1",
    "tsc": "cp src/handlebarsimport.typescript src/handlebarsimport.ts && tsc --project tsconfig.json",
    "dts": "cp src/handlebarsimport.typescript src/handlebarsimport.ts && npx npm-dts generate --output dist/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs.git"
  },
  "keywords": [
    "seismology",
    "earthquake",
    "miniseed",
    "fdsn",
    "iris"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-json": "^4.1.0",
    "@types/d3": "^7.1.0",
    "@types/jest": "^29.0.0",
    "@types/js-quantities": "^1.6.3",
    "@types/leaflet": "^1.7.9",
    "@types/luxon": "^3.0.0",
    "@types/querystringify": "^2.0.0",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-jest": "^29.0.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "documentation": "^14.0.0",
    "esbuild": "^0.15.6",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.13.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-jsdoc": "^39.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "esmify": "^2.1.1",
    "genversion": "^3.1.1",
    "jest": "^29.0.1",
    "jest-27-expect-message": "^1.1.0",
    "jest-environment-jsdom": "^29.0.1",
    "jest-extended": "^3.1.0",
    "node-fetch": "^2.6.7",
    "npm-dts": "^1.3.12",
    "prismjs": "^1.26.0",
    "replace-in-file": "^6.3.2",
    "rollup": "^2.66.1",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.6.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 1% in US",
              "not ie 11",
              "not dead"
            ],
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
